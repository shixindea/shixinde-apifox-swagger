{
  "name": "@shixinde/apifox-swagger",
  "version": "1.0.0",
  "author": "袁果锅",
  "type": "module",
  "description": "果锅工具类",
  "keywords": [
    "@shixinde",
    "袁果锅",
    "apifox-swagger",
    "apifox",
    "swagger",
    "apifox-swagger-tool"
  ],
  "scripts": {
    "dev": "vite",
    "gitcz": "git-cz",
    "start": "vite",
