  "files": [
    "dist/**/*",
    "package.json",
    "readme.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vuepress/bundler-vite": "2.0.0-rc.14",
    "@vuepress/theme-default": "2.0.0-rc.37",
    "cz-git": "^1.9.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vue": "^3.4.31",
    "vuepress": "2.0.0-rc.14"
  },
  "publishConfig": {
    "access": "public"
